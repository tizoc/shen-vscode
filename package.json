{
    "name": "shen",
    "displayName": "Shen",
    "description": "Shen language support for Visual Studio Code",
    "version": "0.0.2",
    "publisher": "tizoc",
    "repository": "https://github.com/tizoc/shen-vscode.gitt",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "shen",
                "aliases": [
                    "Shen",
                    "shen"
                ],
                "extensions": [
                    ".shen"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "shen",
                "scopeName": "source.shen",
                "path": "./syntaxes/shen.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "shen",
                "path": "snippets/shen.json"
            }
        ]
    }
}